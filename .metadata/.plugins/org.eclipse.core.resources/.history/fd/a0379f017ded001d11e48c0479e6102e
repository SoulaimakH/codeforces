package love_story;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        System.out.println(n);
        int[]tab= new int[10];
        String code="codeforces";
        for(int i=0; i<n ;i++) {
        	System.out.println(i);
        	scan.nextLine();
        	String chaine = scan.nextLine();
        	
        	if(chaine.compareTo(code)==0)
        		tab[i]=0;
        	else {
        		System.out.println("else ");
        		for (int c=0; c<9 ;c++) {
        			System.out.println("sqd "+c+" cart= "+code.charAt(c)+";o="+chaine.charAt(c));
        			if(code.charAt(c)!=chaine.charAt(c)) {
        				tab[i]++;
        			}
        		}
        	}
        	
        	
        }
        
        for(int i=0; i <n; i++)  
        {  
        System.out.println(tab[i]);  
        }  
	}

}
