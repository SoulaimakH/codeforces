package love_story;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		int[] tab = new int[n];
		String code = "codeforces";
		for (int i = 0; i < n; i++) {
		    scan.nextLine();
		    String chaine = scan.nextLine().trim();
		    System.out.println("input = " + chaine);
		    if (chaine.equals(code)) {
		        tab[i] = 0;
		    } else {
		        int len = Math.min(chaine.length(), code.length());
		        for (int c = 0; c < len; c++) {
		            if (code.charAt(c) != chaine.charAt(c)) {
		                tab[i]++;
		            }
		        }
		    }
		}
		for (int i = 0; i < n; i++) {
		    System.out.println(tab[i]);
		}


	}

}
